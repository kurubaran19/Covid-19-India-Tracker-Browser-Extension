{
    "name": "COVID-19 INDIA | Tracker",
    "description": "A crowdsourced initiative.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
      ],
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js":["script.js"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "COVID-19 INDIA"
    },
    "icons": {
         "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" 
    }
  }